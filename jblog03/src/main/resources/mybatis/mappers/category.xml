<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="category">
<select id='findById' resultType="categoryvo">
		<![CDATA[
		select *
		  from category
		   where blog_id=#{id }
		]]>
	</select>
</mapper>